        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/gateway/contacts",
      "UpstreamHttpMethod": [ "POST","GET"]
    },
    {
      "DownstreamPathTemplate": "/api/contacts/{id}",
